package lib1

abstract class AbstractClassWithFunctions {
    fun foo(): Int = 42
    open fun bar(): Int = 42
    open fun baz(): Int = 42
}

interface InterfaceWithFunctions {
    fun foo(): Int = 42
    fun bar(): Int = 42
}

abstract class AbstractClassWithProperties {
    konst foo1: Int = 42
    konst foo2: Int get() = 42
    open konst bar1: Int = 42
    open konst bar2: Int get() = 42
    open konst baz1: Int = 42
    open konst baz2: Int get() = 42
}

interface InterfaceWithProperties {
    konst foo: Int get() = 42
    konst bar: Int get() = 42
}
