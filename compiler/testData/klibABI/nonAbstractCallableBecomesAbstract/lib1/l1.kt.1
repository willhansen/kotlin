package lib1

abstract class AbstractClassWithFunctions {
    abstract fun foo(): Int
    abstract fun bar(): Int
    abstract fun baz(): Int
}

interface InterfaceWithFunctions {
    fun foo(): Int
    fun bar(): Int
}

abstract class AbstractClassWithProperties {
    abstract konst foo1: Int
    abstract konst foo2: Int
    abstract konst bar1: Int
    abstract konst bar2: Int
    abstract konst baz1: Int
    abstract konst baz2: Int
}

interface InterfaceWithProperties {
    konst foo: Int
    konst bar: Int
}
